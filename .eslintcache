el htmlFor=\"field_name\">Metodo<span className=\"text-danger\">*</span></Label>\n                                                            <FieldSelect\n                                                                id={\"deliveryMethod\"}\n                                                                name={\"deliveryMethod\"}\n                                                                options={deliveryMethodList}\n                                                                defaultValue={deliveryMethod?.code}\n                                                                onChange={item => setDeliveryMethod(item.value)}\n                                                                required\n                                                            />\n                                                        </div>\n                                                    </Col>\n                                                </Row>\n                                                <Row>\n                                                    <Col md=\"12\">\n                                                        <div className=\"mb-3\">\n                                                            <Label htmlFor=\"field_name\">Descripcin</Label>\n                                                            <FieldText\n                                                                type={\"textarea\"}\n                                                                id={\"description\"}\n                                                                name={\"description\"}\n                                                                value={officeData.description}\n                                                                minLength={3}\n                                                                maxLength={255}\n                                                            />\n                                                        </div>\n      